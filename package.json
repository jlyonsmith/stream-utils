{
  "name": "@johnls/stream-utils",
  "version": "1.0.16",
  "description": "A collection of stream utilities",
  "main": "dist/index.js",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "build": "babel src -d dist --source-maps --ignore **/*.test.js",
    "test": "jest --coverage",
    "deploy": "npm publish --access public",
    "show-coverage": "open coverage/lcov-report/index.html"
  },
  "jest": {
    "collectCoverage": true,
    "testPathIgnorePatterns": [
      "node_modules/",
      "scratch/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/jlyonsmith/stream-utils.git"
  },
  "keywords": [
    "stream",
    "util",
    "string"
  ],
  "author": "John Lyon-Smith",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jlyonsmith/stream-utils/issues"
  },
  "homepage": "https://github.com/jlyonsmith/stream-utils#readme",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/node": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "@babel/plugin-syntax-object-rest-spread": "^7.8.3",
    "@babel/preset-env": "^7.9.6",
    "jest-cli": "^26.0.1"
  }
}
